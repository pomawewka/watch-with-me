  "license": "ISC",
  "dependencies": {
    "cookie-session": "^1.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.18.2",
    "mongoose": "^5.9.6",
    "multer": "^1.4.2",
    "passport": "^0.4.1",
    "passport-discord": "^0.1.3",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth": "^2.0.0",
    "sharp": "^0.25.2",
    "uniqid": "^5.2.0",
    "webtorrent": "^0.108.1"
  },
  "devDependencies": {
    "concurrently": "^5.1.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.2"
  }
